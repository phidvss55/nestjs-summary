### ENV
NODE_ENV="development"
PORT=5000

### DATABASE CONNECTION
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=
POSTGRES_USER=
POSTGRES_PASSWORD=

### DATABASE CONFIG
DATABASE_SYNC=true

### JWT
JWT_SECRET=""
JWT_EXPIRATION_TIME=3600
JWT_REFRESH_SECRET=""
JWT_REFRESH_EXPIRATION_TIME=3600

